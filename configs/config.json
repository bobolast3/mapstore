{
  "header": {
    "isVisible": true,
    "logo": {
      "link": "https://www.ocims.gov.za/"
    },
    "title": "coastal flood hazard",
    "search": {
      "title": "Search By Location",
      "tooltipTitle": "search",
      "iconName": "search"
    },
    "menu": {
      "isOpen": false,
      "isOverlayOpen": false,
      "isDraggable": false,
      "options": [
        {
          "title": "Disclaimer",
          "iconName": "exclamation-sign",
          "componentName": "Disclaimer"
