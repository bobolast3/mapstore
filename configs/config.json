        {
          "title": "About",
          "iconName": "info-sign",
          "componentName": "About"
        }
      ]
    }
  },
  "searchAndRunForm": {
    "isOpen": true,
    "isCollapsed": true,
    "allowClose": false,
    "isDraggable": true,
    "title": "search and rescue",
    "subtitle": "Please input georgraphical coordintates for a location above or drop a pin on the map and select a date and time to run the search algorithm for the location",
    "form": [
      {
        "title": "step 1: select location",
        "inputs": [
          {
            "label": "Coordinate Format",
            "name": "coordinate",
            "select": true,
            "options": [
              { "label": "DMS", "value": "dms" },
              { "label": "Decimal Minutes", "value": "decimal" }
            ],
            "defaultValue": "one two",
            "length": 12
          },
          {
            "label": "Latitude",
            "name": "lat",
            "defaultValue": "one two",
            "length": 6
          },
          {
            "label": "Longitude",
            "name": "lng",
            "defaultValue": "ne twoo",
            "length": 6
          }
        ]
      },
      {
        "title": "step 2: select weather source",
        "inputs": [
          {
            "label": "Weather Source",
            "name": "weatherSource",
            "select": true,
            "options": [
              { "label": "GFS", "value": "gfs" },
              { "label": "TFS", "value": "tfs" }
            ],
            "defaultValue": "one two",
            "length": 6
          }
        ]
      },
      {
        "title": "step 3: select date and time",
        "inputs": [
          {
            "label": "Last Known Date",
            "name": "lastDate",
            "type": "date",
            "InputLabelProps": { "shrink": true },
            "length": 6
          },
          {
            "label": "Recuse Unit: ETA",
            "name": "eta",
            "type": "date",
            "InputLabelProps": { "shrink": true },
            "length": 6
          }
        ]
      },
      {
        "title": "step 4: select target type",
        "inputs": [
          {
            "label": "target type",
            "name": "targetType",
            "select": true,
            "options": [
              {
                "label": "Person in water: state unknown",
                "value": "unknownState"
              },
              { "label": "Person found", "value": "found" }
            ],
            "defaultValue": "one two",
            "length": 12
          }
        ]
      }
    ]
  },
